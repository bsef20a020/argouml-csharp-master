<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.argouml.app.codeGenerator">
      <language
            id="org.argouml.language.csharp"
            name="C#">
      </language>
      <generator
            class="org.argouml.language.csharp.generator.GeneratorCSharp"
            id="org.argouml.language.csharp.generator"
            language="org.argouml.language.csharp"
            name="C# Generator">
      </generator>
   </extension>
   <extension
         point="org.argouml.app.sourceImporter">
      <language
            id="org.argouml.language.csharp"
            name="C#">
      </language>
      <importer
            class="org.argouml.language.csharp.importer.CSharpImport"
            id="org.argouml.language.csharp.importer"
            language="org.argouml.language.csharp"
            name="C# Source Importer (Reverse Engineering)">
      </importer>
   </extension>

</plugin>
